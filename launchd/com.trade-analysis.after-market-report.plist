<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.trade-analysis.after-market-report</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/takueisaotome/PycharmProjects/claude-market-agents/scripts/run_after_market_report.sh</string>
    </array>

    <!-- 平日 13:10 PT (1:10 PM) に実行 -->
    <key>StartCalendarInterval</key>
    <array>
        <!-- Monday -->
        <dict>
            <key>Weekday</key>
            <integer>1</integer>
            <key>Hour</key>
            <integer>13</integer>
            <key>Minute</key>
            <integer>10</integer>
        </dict>
        <!-- Tuesday -->
        <dict>
            <key>Weekday</key>
            <integer>2</integer>
            <key>Hour</key>
            <integer>13</integer>
            <key>Minute</key>
            <integer>10</integer>
        </dict>
        <!-- Wednesday -->
        <dict>
            <key>Weekday</key>
            <integer>3</integer>
            <key>Hour</key>
            <integer>13</integer>
            <key>Minute</key>
            <integer>10</integer>
        </dict>
        <!-- Thursday -->
        <dict>
            <key>Weekday</key>
            <integer>4</integer>
            <key>Hour</key>
            <integer>13</integer>
            <key>Minute</key>
            <integer>10</integer>
        </dict>
        <!-- Friday -->
        <dict>
            <key>Weekday</key>
            <integer>5</integer>
            <key>Hour</key>
            <integer>13</integer>
            <key>Minute</key>
            <integer>10</integer>
        </dict>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/takueisaotome/PycharmProjects/claude-market-agents</string>

    <key>StandardOutPath</key>
    <string>/Users/takueisaotome/PycharmProjects/claude-market-agents/logs/launchd_after_market.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/takueisaotome/PycharmProjects/claude-market-agents/logs/launchd_after_market_error.log</string>

    <!-- ユーザーがログインしている時のみ実行 -->
    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
